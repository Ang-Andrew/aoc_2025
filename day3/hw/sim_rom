    %end;
    .thread T_2;
    .scope S_0x400008ce60;
T_3 ;
    %wait E_0x400008abf0;
    %load/vec4 v0x40000a5540_0;
    %nor/r;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_3.2, 9;
    %load/vec4 v0x40000a54a0_0;
    %pad/u 32;
    %cmpi/u 200, 0, 32;
    %flag_get/vec4 5;
    %and;
T_3.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %load/vec4 v0x40000a54a0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x40000a54a0_0, 0;
T_3.0 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x400008ccd0;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x40000a5710_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x400008ccd0;
T_5 ;
    %delay 2000, 0;
    %load/vec4 v0x40000a5710_0;
    %inv;
    %store/vec4 v0x40000a5710_0, 0, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x400008ccd0;
T_6 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x40000a58e0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x40000a5800_0, 0, 32;
    %wait E_0x400008abf0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x40000a58e0_0, 0, 1;
    %wait E_0x400008abf0;
    %pushi/vec4 250, 0, 32;
T_6.0 %dup/vec4;
    %cmpi/s 0, 0, 32;
    %jmp/1xz T_6.1, 5;
    %jmp/1 T_6.1, 4;
    %subi 1, 0, 32;
    %wait E_0x400008abf0;
    %jmp T_6.0;
T_6.1 ;
    %pop/vec4 1;
    %pushi/vec4 250, 0, 32;
    %store/vec4 v0x40000a5800_0, 0, 32;
    %vpi_call 2 34 "$display", "========================================" {0 0 0};
    %vpi_call 2 35 "$display", "Final Score (decimal): %0d", v0x40000a59b0_0 {0 0 0};
    %vpi_call 2 36 "$display", "Expected (decimal):    17092" {0 0 0};
