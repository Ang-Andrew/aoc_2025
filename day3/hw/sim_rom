v0x4000078cc0_0 .var "data", 31 0;
v0x4000077d00 .array "memory", 255 0, 31 0;
E_0x400008abf0 .event posedge, v0x4000078a60_0;
    .scope S_0x400004fd40;
T_0 ;
    %vpi_call 4 14 "$readmemh", P_0x4000076620, v0x4000077d00 {0 0 0};
