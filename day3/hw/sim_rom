    %vpi_call 2 39 "$display", "========================================" {0 0 0};
    %load/vec4 v0x40000a59b0_0;
    %cmpi/e 17092, 0, 32;
    %jmp/0xz  T_6.2, 4;
    %vpi_call 2 42 "$display", "\342\234\223 TEST PASSED!" {0 0 0};
    %vpi_call 2 43 "$finish" {0 0 0};
    %jmp T_6.3;
T_6.2 ;
    %vpi_call 2 45 "$display", "\342\234\227 TEST FAILED" {0 0 0};
    %pushi/vec4 17092, 0, 32;
    %load/vec4 v0x40000a59b0_0;
    %sub;
