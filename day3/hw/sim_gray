TD_tb_gray_counter.dut.gray_to_binary ;
    %load/vec4 v0x400007d960_0;
    %parti/s 1, 7, 4;
    %ix/load 4, 7, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to gray_to_binary (store_vec4_to_lval)
    %pushi/vec4 6, 0, 32;
    %store/vec4 v0x400007f420_0, 0, 32;
T_0.0 ; Top of for-loop
    %load/vec4 v0x400007f420_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
	  %jmp/0xz T_0.1, 5;
    %retload/vec4 0; Load gray_to_binary (draw_signal_vec4)
    %load/vec4 v0x400007f420_0;
    %addi 1, 0, 32;
    %part/s 1;
    %load/vec4 v0x400007d960_0;
    %load/vec4 v0x400007f420_0;
    %part/s 1;
    %xor;
    %ix/getv/s 4, v0x400007f420_0;
    %ret/vec4 0, 4, 1; Assign to gray_to_binary (store_vec4_to_lval)
T_0.2 ; for-loop step statement
