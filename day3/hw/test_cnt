#! /opt/oss-cad-suite/bin/vvp
:ivl_version "13.0 (devel)" "(s20251012-41-g4f31fec5c)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/opt/oss-cad-suite/lib/ivl/system.vpi";
:vpi_module "/opt/oss-cad-suite/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/oss-cad-suite/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/oss-cad-suite/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/oss-cad-suite/lib/ivl/va_math.vpi";
S_0x400004fe80 .scope module, "test_read_count" "test_read_count" 2 3;
 .timescale -9 -12;
v0x4000076230_0 .var "clk", 0 0;
v0x4000076630_0 .var "read_count", 8 0;
v0x4000076940_0 .var "read_enable", 0 0;
v0x400009dcb0_0 .var "rom_addr", 7 0;
v0x400009dd90_0 .var "rst", 0 0;
E_0x4000050210 .event posedge, v0x4000076230_0;
    .scope S_0x400004fe80;
T_0 ;
    %pushi/vec4 0, 0, 9;
    %store/vec4 v0x4000076630_0, 0, 9;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x400009dcb0_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x4000076940_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x400004fe80;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x4000076230_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x400004fe80;
T_2 ;
    %delay 2000, 0;
    %load/vec4 v0x4000076230_0;
    %inv;
    %store/vec4 v0x4000076230_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x400004fe80;
T_3 ;
    %wait E_0x4000050210;
    %load/vec4 v0x400009dd90_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x400009dcb0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x4000076940_0, 0;
    %pushi/vec4 0, 0, 9;
    %assign/vec4 v0x4000076630_0, 0;
    %jmp T_3.1;
T_3.0 ;
    %load/vec4 v0x4000076630_0;
    %cmpi/u 201, 0, 9;
    %jmp/0xz  T_3.2, 5;
    %load/vec4 v0x4000076630_0;
    %cmpi/u 200, 0, 9;
    %jmp/0xz  T_3.4, 5;
    %load/vec4 v0x4000076630_0;
    %parti/s 8, 0, 2;
    %assign/vec4 v0x400009dcb0_0, 0;
T_3.4 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x4000076940_0, 0;
    %load/vec4 v0x4000076630_0;
    %addi 1, 0, 9;
    %assign/vec4 v0x4000076630_0, 0;
    %jmp T_3.3;
T_3.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x4000076940_0, 0;
T_3.3 ;
T_3.1 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x400004fe80;
T_4 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x400009dd90_0, 0, 1;
    %wait E_0x4000050210;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x400009dd90_0, 0, 1;
    %wait E_0x4000050210;
    %vpi_call 2 35 "$display", "Count | Addr | En" {0 0 0};
    %pushi/vec4 210, 0, 32;
T_4.0 %dup/vec4;
    %cmpi/s 0, 0, 32;
    %jmp/1xz T_4.1, 5;
    %jmp/1 T_4.1, 4;
    %subi 1, 0, 32;
    %wait E_0x4000050210;
    %load/vec4 v0x4000076630_0;
    %pad/u 32;
    %cmpi/u 5, 0, 32;
    %flag_or 5, 4;
    %jmp/1 T_4.4, 5;
    %flag_mov 8, 5;
    %load/vec4 v0x4000076630_0;
    %pad/u 32;
    %cmpi/u 195, 0, 32;
    %flag_inv 5; GE is !LT
    %flag_or 5, 8;
T_4.4;
    %jmp/0xz  T_4.2, 5;
    %vpi_call 2 39 "$display", "%5d | %3d  | %d", v0x4000076630_0, v0x400009dcb0_0, v0x4000076940_0 {0 0 0};
T_4.2 ;
    %jmp T_4.0;
T_4.1 ;
    %pop/vec4 1;
    %vpi_call 2 43 "$finish" {0 0 0};
    %end;
    .thread T_4;
