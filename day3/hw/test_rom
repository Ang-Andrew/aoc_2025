#! /opt/oss-cad-suite/bin/vvp
:ivl_version "13.0 (devel)" "(s20251012-41-g4f31fec5c)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/opt/oss-cad-suite/lib/ivl/system.vpi";
:vpi_module "/opt/oss-cad-suite/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/oss-cad-suite/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/oss-cad-suite/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/oss-cad-suite/lib/ivl/va_math.vpi";
S_0x400008a910 .scope module, "test_rom_only" "test_rom_only" 2 3;
 .timescale -9 -12;
v0x400009d260_0 .var "addr", 7 0;
v0x400009d350_0 .var "clk", 0 0;
v0x400009d420_0 .net "data", 31 0, v0x400009d030_0;  1 drivers
S_0x400008aaa0 .scope module, "rf" "rom_feeder_generic" 2 12, 3 2 0, S_0x400008a910;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 8 "addr";
    .port_info 2 /OUTPUT 32 "data";
P_0x4000075aa0 .param/str "FILENAME" 0 3 3, "data/results.hex";
P_0x4000075ae0 .param/l "WIDTH" 0 3 4, +C4<00000000000000000000000000100000>;
v0x4000077370_0 .net "addr", 7 0, v0x400009d260_0;  1 drivers
v0x400009cf70_0 .net "clk", 0 0, v0x400009d350_0;  1 drivers
v0x400009d030_0 .var "data", 31 0;
v0x400009d120 .array "memory", 255 0, 31 0;
E_0x400008afd0 .event posedge, v0x400009cf70_0;
    .scope S_0x400008aaa0;
T_0 ;
    %vpi_call 3 14 "$readmemh", P_0x4000075aa0, v0x400009d120 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x400008aaa0;
T_1 ;
    %wait E_0x400008afd0;
    %load/vec4 v0x4000077370_0;
    %pad/u 10;
    %ix/vec4 4;
    %load/vec4a v0x400009d120, 4;
    %assign/vec4 v0x400009d030_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x400008a910;
T_2 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x400009d260_0, 0, 8;
    %end;
    .thread T_2;
    .scope S_0x400008a910;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x400009d350_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x400008a910;
T_4 ;
    %delay 2000, 0;
    %load/vec4 v0x400009d350_0;
    %inv;
    %store/vec4 v0x400009d350_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x400008a910;
T_5 ;
    %pushi/vec4 10, 0, 32;
T_5.0 %dup/vec4;
    %cmpi/s 0, 0, 32;
    %jmp/1xz T_5.1, 5;
    %jmp/1 T_5.1, 4;
    %subi 1, 0, 32;
    %wait E_0x400008afd0;
    %vpi_call 2 25 "$display", "Addr=%d, Data=%8h (%d)", v0x400009d260_0, v0x400009d420_0, v0x400009d420_0 {0 0 0};
    %load/vec4 v0x400009d260_0;
    %addi 1, 0, 8;
    %store/vec4 v0x400009d260_0, 0, 8;
    %jmp T_5.0;
T_5.1 ;
    %pop/vec4 1;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "src/test_rom_only.v";
