S_0x400004fe10 .scope module, "test_gray" "test_gray" 2 3;
 .timescale -9 -12;
v0x40000a2d60_0 .net "binary_addr", 7 0, L_0x40000a30d0;  1 drivers
v0x40000a2e60_0 .var "clk", 0 0;
v0x40000a2f20_0 .var "gray_addr", 7 0;
v0x40000a3010_0 .var "rst", 0 0;
E_0x40000506a0 .event posedge, v0x40000a2e60_0;
L_0x40000a30d0 .ufunc/vec4 TD_test_gray.gray_to_binary, 8, v0x40000a2f20_0 (v0x400007add0_0) S_0x400004ffc0;
S_0x400004ffc0 .scope function.vec4.s8, "gray_to_binary" "gray_to_binary" 2 9, 2 9 0, S_0x400004fe10;
 .timescale -9 -12;
v0x400007add0_0 .var "gray", 7 0;
; Variable gray_to_binary is vec4 return value of scope S_0x400004ffc0
v0x40000a27f0_0 .var/i "i", 31 0;
TD_test_gray.gray_to_binary ;
    %load/vec4 v0x400007add0_0;
    %parti/s 1, 7, 4;
    %ix/load 4, 7, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to gray_to_binary (store_vec4_to_lval)
    %pushi/vec4 6, 0, 32;
    %store/vec4 v0x40000a27f0_0, 0, 32;
T_0.0 ; Top of for-loop
    %load/vec4 v0x40000a27f0_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
	  %jmp/0xz T_0.1, 5;
    %retload/vec4 0; Load gray_to_binary (draw_signal_vec4)
    %load/vec4 v0x40000a27f0_0;
    %addi 1, 0, 32;
    %part/s 1;
    %load/vec4 v0x400007add0_0;
    %load/vec4 v0x40000a27f0_0;
    %part/s 1;
    %xor;
    %ix/getv/s 4, v0x40000a27f0_0;
    %ret/vec4 0, 4, 1; Assign to gray_to_binary (store_vec4_to_lval)
T_0.2 ; for-loop step statement
    %load/vec4 v0x40000a27f0_0;
    %subi 1, 0, 32;
    %store/vec4 v0x40000a27f0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ; for-loop exit label
    %end;
S_0x40000a28b0 .scope function.vec4.s8, "increment_gray" "increment_gray" 2 19, 2 19 0, S_0x400004fe10;
 .timescale -9 -12;
v0x40000a2ab0_0 .var "bin", 8 0;
v0x40000a2b90_0 .var "gray", 7 0;
; Variable increment_gray is vec4 return value of scope S_0x40000a28b0
TD_test_gray.increment_gray ;
    %pushi/vec4 0, 0, 1;
    %ix/load 4, 8, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x40000a2ab0_0, 4, 1;
    %load/vec4 v0x40000a2b90_0;
    %parti/s 1, 7, 4;
    %ix/load 4, 7, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x40000a2ab0_0, 4, 1;
    %load/vec4 v0x40000a2b90_0;
    %parti/s 7, 0, 2;
    %pad/u 8;
    %load/vec4 v0x40000a2ab0_0;
    %parti/s 1, 7, 4;
    %load/vec4 v0x40000a2ab0_0;
    %parti/s 7, 1, 2;
    %concat/vec4; draw_concat_vec4
    %xor;
    %pad/u 7;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x40000a2ab0_0, 4, 7;
    %load/vec4 v0x40000a2ab0_0;
    %addi 1, 0, 9;
    %store/vec4 v0x40000a2ab0_0, 0, 9;
    %load/vec4 v0x40000a2ab0_0;
    %parti/s 1, 7, 4;
    %ix/load 4, 7, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to increment_gray (store_vec4_to_lval)
    %load/vec4 v0x40000a2ab0_0;
    %parti/s 7, 1, 2;
    %load/vec4 v0x40000a2ab0_0;
    %parti/s 7, 0, 2;
    %xor;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 7; Assign to increment_gray (store_vec4_to_lval)
    %end;
    .scope S_0x400004fe10;
T_2 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x40000a2f20_0, 0, 8;
    %end;
    .thread T_2;
    .scope S_0x400004fe10;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x40000a2e60_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x400004fe10;
T_4 ;
    %delay 2000, 0;
    %load/vec4 v0x40000a2e60_0;
    %inv;
    %store/vec4 v0x40000a2e60_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x400004fe10;
T_5 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x40000a3010_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x40000a3010_0, 0, 1;
    %pushi/vec4 210, 0, 32;
T_5.0 %dup/vec4;
    %cmpi/s 0, 0, 32;
    %jmp/1xz T_5.1, 5;
    %jmp/1 T_5.1, 4;
    %subi 1, 0, 32;
    %wait E_0x40000506a0;
    %load/vec4 v0x40000a3010_0;
    %nor/r;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_5.4, 9;
    %load/vec4 v0x40000a2d60_0;
    %pad/u 32;
    %cmpi/u 200, 0, 32;
    %flag_get/vec4 5;
    %and;
T_5.4;
    %flag_set/vec4 8;
    %jmp/0xz  T_5.2, 8;
    %load/vec4 v0x40000a2f20_0;
    %store/vec4 v0x40000a2b90_0, 0, 8;
    %callf/vec4 TD_test_gray.increment_gray, S_0x40000a28b0;
    %store/vec4 v0x40000a2f20_0, 0, 8;
    %vpi_func 2 45 "$time" 64 {0 0 0};
    %pushi/vec4 4, 0, 64;
    %div;
    %vpi_call 2 45 "$display", "Cycle %3d: Gray=%3d Binary=%3d", S<0,vec4,u64>, v0x40000a2f20_0, v0x40000a2d60_0 {1 0 0};
T_5.2 ;
    %jmp T_5.0;
T_5.1 ;
    %pop/vec4 1;
    %vpi_call 2 49 "$finish" {0 0 0};
    %end;
    .thread T_5;
