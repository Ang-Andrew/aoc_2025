S_0x400004fe70 .scope module, "test_increment" "test_increment" 2 3;
 .timescale -9 -12;
S_0x4000050000 .scope function.vec4.s8, "increment_gray" "increment_gray" 2 5, 2 5 0, S_0x400004fe70;
 .timescale -9 -12;
v0x40000785d0_0 .var "bin", 8 0;
v0x400009d120_0 .var "gray", 7 0;
v0x400009d200_0 .var/i "i", 31 0;
; Variable increment_gray is vec4 return value of scope S_0x4000050000
TD_test_increment.increment_gray ;
    %pushi/vec4 0, 0, 1;
    %ix/load 4, 8, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x40000785d0_0, 4, 1;
    %load/vec4 v0x400009d120_0;
    %parti/s 1, 7, 4;
    %ix/load 4, 7, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x40000785d0_0, 4, 1;
    %pushi/vec4 6, 0, 32;
    %store/vec4 v0x400009d200_0, 0, 32;
T_0.0 ; Top of for-loop
    %load/vec4 v0x400009d200_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
	  %jmp/0xz T_0.1, 5;
    %load/vec4 v0x400009d120_0;
    %load/vec4 v0x400009d200_0;
    %part/s 1;
    %load/vec4 v0x40000785d0_0;
    %load/vec4 v0x400009d200_0;
    %addi 1, 0, 32;
    %part/s 1;
    %xor;
    %ix/getv/s 4, v0x400009d200_0;
    %store/vec4 v0x40000785d0_0, 4, 1;
T_0.2 ; for-loop step statement
    %load/vec4 v0x400009d200_0;
    %subi 1, 0, 32;
    %store/vec4 v0x400009d200_0, 0, 32;
    %jmp T_0.0;
T_0.1 ; for-loop exit label
    %load/vec4 v0x40000785d0_0;
    %addi 1, 0, 9;
    %store/vec4 v0x40000785d0_0, 0, 9;
    %load/vec4 v0x40000785d0_0;
    %parti/s 1, 7, 4;
    %ix/load 4, 7, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to increment_gray (store_vec4_to_lval)
    %pushi/vec4 6, 0, 32;
    %store/vec4 v0x400009d200_0, 0, 32;
T_0.3 ; Top of for-loop
    %load/vec4 v0x400009d200_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
	  %jmp/0xz T_0.4, 5;
    %load/vec4 v0x40000785d0_0;
    %load/vec4 v0x400009d200_0;
    %addi 1, 0, 32;
    %part/s 1;
    %load/vec4 v0x40000785d0_0;
    %load/vec4 v0x400009d200_0;
    %part/s 1;
    %xor;
    %ix/getv/s 4, v0x400009d200_0;
    %ret/vec4 0, 4, 1; Assign to increment_gray (store_vec4_to_lval)
T_0.5 ; for-loop step statement
    %load/vec4 v0x400009d200_0;
    %subi 1, 0, 32;
    %store/vec4 v0x400009d200_0, 0, 32;
    %jmp T_0.3;
T_0.4 ; for-loop exit label
    %end;
    .scope S_0x400004fe70;
T_1 ;
    %vpi_call 2 25 "$display", "Testing increment_gray:" {0 0 0};
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x400009d120_0, 0, 8;
    %callf/vec4 TD_test_increment.increment_gray, S_0x4000050000;
    %vpi_call 2 26 "$display", "0 -> %d", S<0,vec4,u8> {1 0 0};
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x400009d120_0, 0, 8;
    %callf/vec4 TD_test_increment.increment_gray, S_0x4000050000;
    %vpi_call 2 27 "$display", "1 -> %d", S<0,vec4,u8> {1 0 0};
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x400009d120_0, 0, 8;
    %callf/vec4 TD_test_increment.increment_gray, S_0x4000050000;
    %vpi_call 2 28 "$display", "3 -> %d", S<0,vec4,u8> {1 0 0};
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x400009d120_0, 0, 8;
    %callf/vec4 TD_test_increment.increment_gray, S_0x4000050000;
    %vpi_call 2 29 "$display", "2 -> %d", S<0,vec4,u8> {1 0 0};
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_1;
